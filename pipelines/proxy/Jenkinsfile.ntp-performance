pipeline {
  agent {
      label 'general-performance'
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCode.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./sunbird-devops/ansible/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'METADATA_FILE=./pipelines/proxy/metadata.json ARTIFACT_LABEL=gold  ENV=ntp-performance ./pipelines/proxy/deploy.sh'
        archive includes: "./pipelines/proxy/metadata.json"
      }
    }
  }
}
