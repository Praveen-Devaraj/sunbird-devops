pipeline {
	agent {
		label 'general-staging'
	}
	stages {
		stage('Checkout'){
		   steps {
		   	  sh('./pipelines/setupDevopsCode.sh')
		   }
		}

		stage('Build'){
			steps{
				sh('./sunbird-devops/ansible/installDeps.sh')
			}
		}
		stage('Deploy') {
			steps{
				sh('ansible-playbook -i ansible/inventories/ntp-staging ansible/dockerservice-remove.yml --vault-password-file /run/secrets/vault-pass')
			}
		}
	}
}