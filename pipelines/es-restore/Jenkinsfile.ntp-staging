pipeline {
  agent {
      label 'general-staging'
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCode.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./pipelines/es-restore/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=ntp-staging ./pipelines/es-restore/deploy.sh'
      }
    }
  }
}
