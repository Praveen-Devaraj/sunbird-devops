pipeline {
  agent {
      label 'general-production'
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCode.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./pipelines/es-restore/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=ntp-production ./pipelines/es-restore/deploy.sh'
      }
    }
  }
}
