pipeline {
  agent {
      label 'general-staging'
  }
  stages {
    stage('Checkout') {
      steps {
        sh('./pipelines/setupDevopsCode.sh')
      }
    }
    stage('Pre-Build'){
      steps {
        sh('./pipelines/log-es-restore/installDeps.sh')
      }
    }
    stage('Deploy') {
      steps {
        sh 'ENV=dev ./pipelines/log-es-restore/deploy.sh'
      }
    }
  }
}
